{
  "allow_read": [
    "**/*"
  ],
  "allow_write": [
    "**/*"
  ],
  "allow_bash": [
    "git:*",
    "python:*",
    "pip:*",
    "ls:*",
    "find:*",
    "grep:*",
    "mkdir:*",
    "rm:*",
    "cp:*",
    "mv:*",
    "touch:*",
    "cat:*",
    "head:*",
    "tail:*",
    "wc:*",
    "which:*",
    "chmod:*"
  ],
  "project_context": {
    "name": "AI Benchmark Suite",
    "type": "unified_evaluation_framework",
    "primary_languages": ["python", "yaml"],
    "key_directories": [
      "src/",
      "config/",
      "harnesses/",
      "results/",
      "docs/"
    ],
    "evaluation_harnesses": [
      "bigcode-evaluation-harness",
      "lm-evaluation-harness"
    ],
    "safety_notes": [
      "Harnesses are read-only git submodules",
      "Never edit files in harnesses/ directories",
      "All customizations go in src/ directory"
    ]
  }
}